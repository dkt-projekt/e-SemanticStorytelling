eContext":"http://dkt.dfki.de/documents/bm#char=0,18373","taClassRef":"http://dbpedia.org/ontology/Person"},{"@id":"http://dkt.dfki.de/documents/bm#char=10900,10905","@type":["nif:String","nif:RFC5147String"],"nif:anchorOf":"Peter","beginIndex":"10900","endIndex":"10905","referenceContext":"http://dkt.dfki.de/documents/bm#char=0,18373","taClassRef":"http://dbpedia.org/ontology/Person"},{"@id":"http://dkt.dfki.de/documents/bm#char=10937,10941","@type":["nif:RFC5147String","nif:String"],"nif:anchorOf":"1974","beginIndex":"10937","endIndex":"10941","referenceContext":"http://dkt.dfki.de/documents/bm#char=0,18373","taClassRef":"time:TemporalEntity","intervalFinishes":"1975-01-01T00:00:00","intervalStarts":"1974-01-01T00:00:00"},{"@id":"http://dkt.dfki.de/documents/bm#char=11088,11107","@type":["nif:String","nif:RFC5147String"],"nif:anchorOf":"Crystal Palace Park","beginIndex":"11088","endIndex":"11107","referenceContext":"http://dkt.dfki.de/documents/bm#char=0,18373","taClassRef":"http://dbpedia.org/ontology/Location"},{"@id":"http://dkt.dfki.de/documents/bm#char=11122,11128","@type":["nif:String","nif:RFC5147String"],"nif:anchorOf":"London","beginIndex":"11122","endIndex":"11128","referenceContext":"http://dkt.dfki.de/documents/bm#char=0,18373","taClassRef":"http://dbpedia.org/ontology/Location"},{"@id":"http://dkt.dfki.de/documents/bm#char=11275,11282","@type":["nif:String","nif:RFC5147String"],"nif:anchorOf":"Carlton","beginIndex":"11275","endIndex":"11282","referenceContext":"http://dkt.dfki.de/documents/bm#char=0,18373","taClassRef":"http://dbpedia.org/ontology/Organisation"},{"@id":"http://dkt.dfki.de/documents/bm#char=11353,11359","@type":["nif:String","nif:RFC5147String"],"nif:anchorOf":"Marvin","beginIndex":"11353","endIndex":"11359","referenceContext":"http://dkt.dfki.de/documents/bm#char=0,18373","taClassRef":"http://dbpedia.org/ontology/Person"},{"@id":"http://dkt.dfki.de/documents/bm#char=11364,11375","@type":["nif:String","nif:RFC5147String"],"nif:anchorOf