dex":"10671","endIndex":"10683","referenceContext":"http://dkt.dfki.de/documents/lc#char=0,14986","taClassRef":"http://dbpedia.org/ontology/Person"},{"@id":"http://dkt.dfki.de/documents/lc#char=10732,10739","@type":["nif:String","nif:RFC5147String"],"nif:anchorOf":"Kerouac","beginIndex":"10732","endIndex":"10739","referenceContext":"http://dkt.dfki.de/documents/lc#char=0,14986","taClassRef":"http://dbpedia.org/ontology/Person"},{"@id":"http://dkt.dfki.de/documents/lc#char=10790,10804","@type":["nif:String","nif:RFC5147String"],"nif:anchorOf":"Allen Ginsberg","beginIndex":"10790","endIndex":"10804","referenceContext":"http://dkt.dfki.de/documents/lc#char=0,14986","taClassRef":"http://dbpedia.org/ontology/Person"},{"@id":"http://dkt.dfki.de/documents/lc#char=10896,10904","@type":["nif:String","nif:RFC5147String"],"nif:anchorOf":"Columbia","beginIndex":"10896","endIndex":"10904","referenceContext":"http://dkt.dfki.de/documents/lc#char=0,14986","taClassRef":"http://dbpedia.org/ontology/Organisation"},{"@id":"http://dkt.dfki.de/documents/lc#char=1090,1094","@type":["nif:String","nif:RFC5147String"],"nif:anchorOf":"Carr","beginIndex":"1090","endIndex":"1094","referenceContext":"http://dkt.dfki.de/documents/lc#char=0,14986","taClassRef":"http://dbpedia.org/ontology/Person"},{"@id":"http://dkt.dfki.de/documents/lc#char=10954,10958","@type":["nif:String","nif:RFC5147String"],"nif:anchorOf":"Carr","beginIndex":"10954","endIndex":"10958","referenceContext":"http://dkt.dfki.de/documents/lc#char=0,14986","taClassRef":"http://dbpedia.org/ontology/Person"},{"@id":"http://dkt.dfki.de/documents/lc#char=10988,10996","@type":["nif:String","nif:RFC5147String"],"nif:anchorOf":"Ginsberg","beginIndex":"10988","endIndex":"10996","referenceContext":"http://dkt.dfki.de/documents/lc#char=0,14986","taClassRef":"http://dbpedia.org/ontology/Person"},{"@id":"http://dkt.dfki.de/documents/lc#char=11032,11043","@type":["nif:String","nif:RFC5147String"],"nif:anchorOf":"Bill Morgan","beginIndex":"1103